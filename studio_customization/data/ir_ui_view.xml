    <field name="arch" type="xml">
      <form>
        <header/>
        <sheet string="Items">
          <widget name="web_ribbon" text="Archived" bg_color="bg-danger" attrs="{'invisible': [('x_active', '=', True)]}"/>
          <field name="x_active" invisible="1"/>
          <div class="oe_title">
            <h1>
              <field name="x_name" required="1" placeholder="Name..."/>
            </h1>
          </div>
          <group name="studio_group_48d2bb">
            <group name="studio_group_48d2bb_left"/>
            <group name="studio_group_48d2bb_right"/>
          </group>
        </sheet>
        <div class="oe_chatter" name="oe_chatter">
          <field name="message_follower_ids"/>
          <field name="message_ids"/>
          <field name="activity_ids"/>
        </div>
      </form>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" eval="False"/>
    <field name="key" eval="False"/>
    <field name="mode">primary</field>
    <field name="model">x_items</field>
    <field name="name">Default form view for x_items</field>
    <field name="priority">16</field>
    <field name="type">form</field>
